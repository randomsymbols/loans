{% extends 'base.html.twig' %}

{% block body %}
    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>City</th>
            <th>State</th>
            <th>ZIP Code</th>
            <th>SSN</th>
            <th>FICO</th>
            <th>Wage</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
        </thead>
        <tbody>
        {% for client in clients %}
            <tr>
                <td>{{ client.ClientId }}</td>
                <td>{{ client.firstName }}</td>
                <td>{{ client.lastName }}</td>
                <td>{{ client.age }}</td>
                <td>{{ client.city }}</td>
                <td>{{ client.state }}</td>
                <td>{{ client.zip }}</td>
                <td>{{ client.ssn }}</td>
                <td>{{ client.fico }}</td>
                <td>{{ client.wage }}</td>
                <td>{{ client.email }}</td>
                <td>{{ client.phone }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <div class="row">
        <h1>New Client</h1>
    </div>
    <div class="row">
        {{ form(form) }}
    </div>
{% endblock %}
